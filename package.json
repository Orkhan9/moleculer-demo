    "microservices",
    "moleculer"
  ],
  "author": "",
  "devDependencies": {
    "moleculer-repl": "^0.6.4"
  },
  "dependencies": {
    "moleculer": "^0.14.13",
    "moleculer-web": "^0.9.1",
    "nats": "^1.4.12"
  },
  "engines": {
    "node": ">= 10.x.x"
  }
}
